set nocompatible
set nu rnu 
filetype on
filetype plugin on
filetype indent on
set cursorline
set cursorcolumn
set shiftwidth=4
set tabstop=4
set expandtab
set nobackup
set incsearch
set showcmd
set showmode
set showmatch
set hlsearch
set termguicolors
colorscheme catppuccin_mocha
syntax on
nnoremap ; :

